{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.1",
        "drupal/autologout": "^1.4",
        "drupal/block_field": "^1.0@RC",
        "drupal/bootstrap": "^3.21",
        "drupal/ckeditor": "^1.0",
        "drupal/colorbox": "^2.0",
        "drupal/content_translation_redirect": "^2.0@alpha",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/ctools": "^4.0",
        "drupal/devel": "^5.0",
        "drupal/easy_install": "^10.5",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/external_data_source": "^3.1",
        "drupal/externalauth": "^2.0",
        "drupal/extlink": "^1.3",
        "drupal/field_group": "^3.2",
        "drupal/fontawesome": "^2.25",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^1.4",
        "drupal/honeypot": "^2.1",
        "drupal/imageapi_optimize": "^4.0",
        "drupal/imageapi_optimize_binaries": "^1.0@alpha",
        "drupal/language_switcher_extended": "^1.1",
        "drupal/link_attributes": "^1.11",
        "drupal/linkit": "^6.0",
        "drupal/masonry": "^2.0",
        "drupal/masquerade": "^2.0",
        "drupal/masquerade_log": "^1.2",
        "drupal/matomo": "^1.21",
        "drupal/node_view_permissions": "^1.2",
        "drupal/override_node_options": "^2.7",
        "drupal/paragraphs": "^1.14",
        "drupal/pathauto": "^1.11",
        "drupal/purge": "^3.0",
        "drupal/queue_ui": "^3.1",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.4",
        "drupal/samlauth": "^3.8",
        "drupal/schema_metatag": "^1.4",
        "drupal/search_api_solr": "^4.0",
        "drupal/simple_sitemap": "^4.1",
        "drupal/simplei": "^2.1",
        "drupal/stable": "^2.0",
        "drupal/tablefield": "^2.4",
        "drupal/taxonomy_path_breadcrumb": "^1.0@beta",
        "drupal/token_filter": "^2.0",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/upgrade_status": "^4.0",
        "drupal/varnish_purge": "^2.2",
        "drupal/views_fieldsets": "^4.0",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.2@beta",
        "drush/drush": "^12",
        "mglaman/composer-drupal-lenient": "^1.0",
        "predis/predis": "^1.1",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "wikimedia/composer-merge-plugin": true,
            "cweagans/composer-patches": true,
            "mglaman/composer-drupal-lenient": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "enable-patching": true,
        "patches": {
          "drupal/core": {
            "Allow exposed form to preserve URL query parameters": "https://www.drupal.org/files/issues/2023-04-18/2681953-42.patch"
          },
          "drupal/easy_install": {
            "Easy Install Compat for D10": "https://www.drupal.org/files/issues/2023-04-12/easy_install_d10-3297022-9.patch"
          },
          "drupal/fontawesome": {
            "Fontawesome Compat for D10": "https://www.drupal.org/files/issues/2023-06-05/3274028-fontawesome-ckeditor5-compatibility-16.patch"
          }
        },
        "merge-plugin": {
          "include": [
            "web/modules/contrib/masonry/composer.libraries.json"
          ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/easy_install", "drupal/fontawesome"]
        }
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "drupal/coder": "^8.3",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
